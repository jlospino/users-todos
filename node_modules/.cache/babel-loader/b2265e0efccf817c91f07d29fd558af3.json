{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: \"TableUsersToDos\",\n  props: {\n    userId: String\n  },\n\n  created() {\n    if (!localStorage.setItem(\"todos\")) {\n      this.getUserToDos();\n    }\n\n    this.todos_filter = localStorage.setItem(\"todos\");\n  },\n\n  data() {\n    return {\n      filter: 'all',\n      todos_filter: [],\n      todos: [],\n      todo: ''\n    };\n  },\n\n  methods: {\n    getUserToDos() {\n      axios.get(`https://jsonplaceholder.typicode.com/todos?userId=${this.userId}`).then(res => {\n        this.todos = res.data;\n        localStorage.setItem(\"todos\", JSON.stringify(res.data));\n      });\n    },\n\n    filterToDos(state) {\n      this.filter = state;\n      let todosStorage = localStorage.setItem(\"todos\");\n\n      if (state === 'all') {\n        this.todos_filter = todosStorage;\n      } else {\n        this.todos_filter = todosStorage.filter(function (todo) {\n          return state === 'pending' ? todo.completed == false : todo.completed == true;\n        });\n      }\n    },\n\n    addToDo() {\n      if (this.todo === '') return;\n      axios.post(`https://jsonplaceholder.typicode.com/todos?userId=${this.userId}`, {\n        completed: false,\n        title: this.todo,\n        userId: this.userId\n      }).then(res => {\n        console.log(res);\n        this.getUserToDos();\n      });\n    },\n\n    removeToDo(id) {\n      axios.delete(`https://jsonplaceholder.typicode.com/todos/${id}`).then(res => {\n        console.log(res);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsDA;AACA;EACAA,uBADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACA;IACA;;IACA;EACA,CAVA;;EAWAC;IACA;MACAC,aADA;MAEAC,gBAFA;MAGAC,SAHA;MAIAC;IAJA;EAMA,CAlBA;;EAmBAC;IACAC;MACAC,8EACAC,IADA,CACAC;QACA;QACAC;MAEA,CALA;IAMA,CARA;;IASAC;MACA;MACA;;MACA;QACA;MACA,CAFA,MAEA;QACA;UACA;QACA,CAFA;MAGA;IACA,CAnBA;;IAoBAC;MACA;MACAL;QACAM,gBADA;QAEAC,gBAFA;QAGAhB;MAHA,GAKAU,IALA,CAKAC;QACAM;QACA;MACA,CARA;IAUA,CAhCA;;IAi