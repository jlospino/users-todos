{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: \"TableUsersToDos\",\n  props: {\n    userId: String\n  },\n\n  created() {\n    this.getUserToDos();\n  },\n\n  data() {\n    return {\n      filter: 'all',\n      tasks: [],\n      todos: []\n    };\n  },\n\n  methods: {\n    getUserToDos() {\n      axios.get(`https://jsonplaceholder.typicode.com/todos?userId=${this.userId}`).then(res => {\n        this.todos = res.data;\n      });\n    },\n\n    filterToDos(state) {\n      this.filter = state;\n\n      if (state === 'all') {\n        this.tasks = this.todos;\n        console.log(this.tasks);\n      }\n\n      if (state === 'pending') {\n        this.tasks = this.todos.filter(function (todo) {\n          return todo.completed == false;\n        });\n      }\n\n      if (state === 'completed') {\n        this.tasks = this.todos.filter(function (todo) {\n          return todo.completed == true;\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CA;AACA;EACAA,uBADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;EACA,CAPA;;EAQAC;IACA;MACAC,aADA;MAEAC,SAFA;MAGAC;IAHA;EAKA,CAdA;;EAeAC;IACAC;MACAC;QACA;MACA,CAFA;IAGA,CALA;;IAMAC;MACA;;MACA;QACA;QACAC;MACA;;MACA;QACA;UACA;QACA,CAFA;MAGA;;MACA;QACA;UACA;QACA,CAFA;MAGA;IACA;;EAtBA;AAfA","names":["name","props","userId","created","data","filter","tasks","todos","methods","getUserToDos","axios","filterToDos","console"],"sourceRoot":"src/components/users","sources":["TableUserToDos.vue"],"sourcesContent":["<template>\r\n    <div class=\"todos-list\">\r\n        <div class=\"task-input\">\r\n            <span class=\"material-symbols-outlined task-input-icon\">\r\n                splitscreen\r\n            </span>\r\n            <input type=\"text\" placeholder=\"Add a new task\">\r\n            <span>{{ tasks.length }}</span>\r\n        </div>\r\n        <div class=\"controls\">\r\n            <div class=\"filters\">\r\n                <span \r\n                    :class=\"{'active': filter == 'all'}\"\r\n                    @click=\"filterToDos('all')\"\r\n                >\r\n                    All\r\n                </span>\r\n                <span \r\n                    :class=\"{'active': filter == 'pending'}\"\r\n                    @click=\"filterToDos('pending')\"\r\n                >\r\n                    Pending\r\n                </span>\r\n                <span \r\n                    :class=\"{'active': filter == 'completed'}\"\r\n                    @click=\"filterToDos('completed')\"\r\n                >\r\n                    Completed\r\n                </span>\r\n            </div> \r\n        </div>\r\n        <ul class=\"task-box\">\r\n            <li class=\"task\" v-for=\"(todo, index) in tasks\" :key=\"index\">\r\n                <label :for=\"`${todo.id}`\">\r\n                    <input type=\"checkbox\" :id=\"`${todo.id}`\" />\r\n                    <p :class=\"`${todo.completed}`\">{{ todo.title }}</p>\r\n                </label>\r\n                <button class=\"button_small\">Eliminar</button>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n    name: \"TableUsersToDos\",\r\n    props:{\r\n        userId: String\r\n    },\r\n    created(){\r\n        this.getUserToDos();\r\n    },\r\n    data() {\r\n        return {\r\n            filter: 'all',\r\n            tasks: [],\r\n            todos: [],\r\n        }\r\n    },\r\n    methods:{\r\n        getUserToDos() {\r\n            axios.get(`https://jsonplaceholder.typicode.com/todos?userId=${this.userId}`).then((res) => {\r\n                this.todos = res.data;\r\n            });\r\n        },\r\n        filterToDos(state){\r\n            this.filter = state;\r\n            if(state === 'all'){\r\n                this.tasks = this.todos;\r\n                console.log(this.tasks)\r\n            }\r\n            if(state === 'pending'){\r\n                this.tasks = this.todos.filter( function(todo) {\r\n                    return  todo.completed == false;\r\n                });\r\n            }\r\n            if(state === 'completed'){\r\n                this.tasks = this.todos.filter( function(todo) {\r\n                    return  todo.completed == true;\r\n                });\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}